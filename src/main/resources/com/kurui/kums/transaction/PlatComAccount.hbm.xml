 {
     this.index=index;
 }
 




]]></meta>


		<id name="id" type="long" column="id">
			<generator class="sequence">
				<param name="sequence">seq_platcomaccount</param>
			</generator>
		</id>

		<property name="type" column="type" type="java.lang.Long"
			not-null="false" insert="true" update="true">
		</property>
		<property name="description" column="description" type="string"
			not-null="false" insert="true" update="true">
		</property>
		<property name="status" column="status" type="java.lang.Long"
			not-null="false" insert="true" update="true">
		</property>
		<property name="updateTime" column="update_time"
			type="java.sql.Timestamp" not-null="false" insert="true"
			update="true">
		</property>
		<property name="userName" column="user_name" type="string"
			not-null="false" insert="true" update="true">
		</property>



		<many-to-one name="platform"
			class="com.kurui.kums.transaction.Platform" not-null="false">

			<column name="platform_id" />
		</many-to-one>
		<many-to-one name="company"
			class="com.kurui.kums.transaction.Company" not-null="false">

			<column name="company_id" />
		</many-to-one>
		<many-to-one name="account"
			class="com.kurui.kums.transaction.Account" not-null="false">

			<column name="account_id" />
		</many-to-one>
	</class>

</hibernate-mapping>